<?xml version="1.0" encoding="UTF-8"?>
<view>
    <columns>
        <group name="no-visible" numcolumns="12">
            <column name="idasientopre" display="none" order="100">
                <widget type="text" fieldname="idasientopre" readonly="true"/>
            </column>
        </group>
        
        <group name="data" numcolumns="12">
            <column name="code" numcolumns="2" order="100">
                <widget type="text" fieldname="codigo" maxlength="1" required="true"/>
            </column>
            <column name="Mensaje" numcolumns="10" order="130">
                <widget type="text" fieldname="mensaje" maxlength="255" required="true" />
            </column>
        </group>
    </columns>
</view>